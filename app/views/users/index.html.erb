<h1 class="my-3">ユーザー一覧</h1>

<div class="mb-3">
  <table class="table table-hover">
    <thead class="tehad-default">
      <tr>
        <th>ユーザー名</th>
        <th>メールアドレス</th>
        <th>登録日時</th>
      </tr>
    </thead>
    <tbody>
      <% @user.each do |u| %>
        <tr>
          <td><%= u.name %></td>
          <td><%= u.email %></td>
          <td><%= u.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
